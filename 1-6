using System;
class Program
{
    static void Main(string[] args)
    {
        int[] A = new int[101];
        for (int i = 0; i <A.Length; i++)
        {
            A[i] = i + 100;
            while (A[i] % 3 == 0)
            {
                Console.WriteLine(A[i]);
                break;
            }
        }
    }
}
